529296"""
enc=enc.replace(b'\n',b'')
enc=binascii.unhexlify(enc)
print('type enc: ', type(enc))
intro="The following encoded individuals are to be given a $27.3k bonus:".ljust(63) + '\n'
d=intro.encode('utf-8')
print(d)
print(len(d))
otp=[]
for i in range(0, 64):
    # print('type char? :', type(d[i%64] ^ enc[i]))
    otp.append(d[i] ^ enc[i])
otp = bytes(otp)
print('type otp: ', type(otp))
text=[]
for i in range(len(enc)):
    text.append((otp[i%64] ^ enc[i]))
print(bytes(text))
